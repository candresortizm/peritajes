<h1>Nuevo proceso de documentación</h1>
<%= form_tag(new_car_process_path, method: "get") do %>
  <%= label_tag(:q, "Ingrese la placa del vehìculo:") %>
  <%= text_field_tag(:plate, nil,required: true, pattern: "[A-Z]{3}[0-9]{3}", class:"plate_input") %></br>
  <%= label_tag(:q, "Ingrese el proceso de documentación:") %>
  <%= select_tag :process_type, options_for_select(@documentation_types) %></br>
  <%= submit_tag("Buscar") %>
<% end %>
